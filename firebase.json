{
  "hosting": {
    "public": "out",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "websites",
        "destination": "websites"
      },
      {
        "source": "fitness",
        "destination": "fitness"
      },
      {
        "source": "blog",
        "destination": "blog"
      },
      {
        "source": "signin",
        "destination": "signin"
      },
      {
        "source": "signup",
        "destination": "signup"
      },
      {
        "source": "blog/:slug",
        "destination": "blog/[slug]"
      },
      {
        "source": "**",
        "destination": "/index"
      }
    ]
  }
}
